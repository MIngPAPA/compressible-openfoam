FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0]; // kg/m^3

internalField   uniform 1.228;

boundaryField
{
    outlet
    {
        type            zeroGradient;
        freeStreamValue 1.0;
    }
    inlet
    {
        type            zeroGradient;
    }
    wall
    {
        type            slip;
    }
    far_field
    {
        type            zeroGradient;
    }
}


